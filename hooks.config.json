    },
    "openai": {
      "voice": "nova",
      "model": "gpt-4o-mini-tts"
    },
    "unrealSpeech": {
      "voice": "Chloe",
      "temperature": 0.25
    },
    "templateTone": "concise"
  },
  "templates": {
    "notification": {
      "withName": "{{projectName}}: {{userName}}, your super agent needs your input",
      "withoutName": "{{projectName}}: Your agent needs your input"
