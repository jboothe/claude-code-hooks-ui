    "nameIncludeProbability": 1,
    "hookToggles": {
      "stop": true,
      "subagentStop": true,
      "notification": true,
      "sessionEnd": true
    },
    "queue": {
      "enabled": true,
      "maxWaitMs": 30000
    },
    "native": {
      "voice": "Samantha",
      "rate": 180
    },
    "elevenlabs": {
      "voiceId": "XrExE9yKIg1WjnnlVkGX",
